# import a csv file to jupyter
# pandas helps to read files in different format
# numpy helps to convert data in arrays or matrix

# import a csv file

import pandas as pd
import numpy as np

data = pd.read_csv("filename.csv", delimiter=";")
print(data.head())

datatoarray = np.array(data)



#import a xlsx file 

import pandas as pd

data = pd.read_excel("filename.xlsx", sheet_name=4)
print(data.head())

datatoarray = np.array(data)
